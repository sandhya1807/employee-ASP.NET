@model IEnumerable<employee.model.Employ>
@{
    Layout = "~/Views/shared/-Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h1> Employ Details</h1>
    <table border="1">
        <tr>
            <td>ID</td>
            <td>NAME</td>
            <td>EMail</td>
            <td>dept</td>
        </tr>
        @foreach (var emp in Model)
        {
            <tr>
                <td>@emp.Id</td>
                <td>@emp.Name</td>
                <td>@emp.Email</td>
                <td>@emp.dept</td>
            </tr>
         }

    </table>
</body>
</html>
